<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_home"
    tools:context=".Activity.adminHome">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.57" />

    <ImageView
        android:id="@+id/imgLogoutA"
        android:layout_width="56dp"
        android:layout_height="69dp"
        android:layout_marginTop="16dp"
        android:tint="#EDCFE0"
        app:layout_constraintEnd_toStartOf="@+id/txtLogoutA"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_logout_24" />

    <TextView
        android:id="@+id/txtLogoutA"
        android:layout_width="83dp"
        android:layout_height="34dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Logout"
        android:textColor="#EED0E1"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="359dp"
        android:layout_height="244dp"
        android:layout_marginTop="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fulllogo" />

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cardView2"
        style="@style/custom_card_view_style"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#FEF2F4"
        android:backgroundTint="#EFD1E2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline3">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgCategoryA"
                android:layout_width="94dp"
                android:layout_height="75dp"
                android:layout_marginStart="52dp"
                android:layout_marginBottom="8dp"
                android:tint="#875B72"
                app:layout_constraintBottom_toTopOf="@+id/textView6"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/baseline_cart_24" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="113dp"
                android:layout_height="31dp"
                android:layout_marginBottom="16dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Category"
                android:textColor="#875B72"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView16"
                app:layout_constraintHorizontal_bias="0.472"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="93dp"
                android:layout_height="31dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Flowers"
                android:textColor="#875B72"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/imgCategoryA"
                app:layout_constraintEnd_toStartOf="@+id/textView18"
                app:layout_constraintHorizontal_bias="0.417"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageView
                android:id="@+id/imgProductA"
                android:layout_width="89dp"
                android:layout_height="78dp"
                android:tint="#875B72"
                app:layout_constraintBottom_toTopOf="@+id/textView11"
                app:layout_constraintEnd_toStartOf="@+id/imgUserA"
                app:layout_constraintHorizontal_bias="0.424"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.853"
                app:srcCompat="@drawable/baseline_local_florist_24" />

            <ImageView
                android:id="@+id/imgFlowerA"
                android:layout_width="98dp"
                android:layout_height="83dp"
                android:tint="#875B72"
                app:layout_constraintBottom_toTopOf="@+id/textView16"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.55"
                app:layout_constraintStart_toEndOf="@+id/imgCategoryA"
                app:srcCompat="@drawable/baseline_shopping_bag_24" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="105dp"
                android:layout_height="30dp"
                android:layout_marginEnd="44dp"
                android:layout_marginBottom="12dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Product"
                android:textColor="#875B72"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

            <ImageView
                android:id="@+id/imgUserA"
                android:layout_width="102dp"
                android:layout_height="87dp"
                android:layout_marginEnd="60dp"
                android:tint="#875B72"
                app:layout_constraintBottom_toTopOf="@+id/textView18"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.812"
                app:srcCompat="@drawable/baseline_person_24" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="68dp"
                android:layout_height="31dp"
                android:layout_marginEnd="72dp"
                android:layout_marginBottom="12dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Users"
                android:textColor="#875B72"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/imgFlowerA"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.card.MaterialCardView>

</androidx.constraintlayout.widget.ConstraintLayout>